@model IEnumerable<MVCBlogLab.Models.Article>
@{
    ViewBag.Title = "新增留言";
}

<h2>Create</h2>
@using (Html.BeginForm("Create", "Message"))
{
    <div>
        @Html.Label("標題")
        @Html.TextBox("ArticleTitle")
        <br />
        @Html.Label("內容")
        @Html.TextArea("Content")
        <br />
        <input type="submit" value=" 送出留言 " />
    </div>
}

